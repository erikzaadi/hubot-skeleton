{
  "name": "hubot-skeleton",
  "version": "1.0.0",
  "description": "a simple skeleton for an empty shell hubot instance",
  "main": "./node_modules/.bin/coffee ./node_modules/.bin/hubot",
  "scripts": {
    "bot": "./node_modules/.bin/coffee ./node_modules/.bin/hubot",
    "test": "./node_modules/.bin/mocha --recursive --compilers 'coffee:coffee-script' ./tests/*"
  },
  "keywords": [
    "hubot"
  ],
  "author": "erik.zaadi@gmail.com",
  "license": "MIT",
  "dependencies": {
    "coffee": "2.1.0",
    "hubot": "2.17.0",
    "hubot-help": "0.1.2",
    "hubot-redis-brain": "0.0.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "hubot-test-helper": "1.4.4",
    "mocha": "2.4.5"
  }
}
